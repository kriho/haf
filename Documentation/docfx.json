{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "Documentation"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "articles/*.md",
          "articles/toc.yml",
          "*.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "annotations/*.md"
        ]
      }
    ],
    "dest": "site",
    "template": [
      "statictoc",
      "templates/customstatictoc"
    ],
    "globalMetadata": {
      "_appTitle": "HAF",
      "_appFooter": "haf.hornung.dev",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/kriho/haf",
        "branch": "master",
        "apiSpecFolder": "annotations"
      }
    },
    "markdownEngineName": "markdig"
  }
}